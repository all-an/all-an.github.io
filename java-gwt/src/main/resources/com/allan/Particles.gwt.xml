<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.11.0//EN"
        "http://gwtproject.org/doctype/2.11.0/gwt-module.dtd">
<module rename-to="particles">
    <!-- Inherit the core Web Toolkit stuff -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the Canvas module for HTML5 canvas support -->
    <inherits name='com.google.gwt.canvas.Canvas'/>

    <!-- Entry point class -->
    <entry-point class='com.allan.client.Particles'/>

    <!-- Specify the app servlets -->
    <source path='client'/>
</module>
